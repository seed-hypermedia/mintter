{
  "name": "@mintter/ui",
  "version": "0.1.0",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./types/index",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "type": "module",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json,js}\" --check --ignore-path ../../../.prettierignore",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "test": "echo NO TESTS",
    "typecheck": "tsc --noEmit",
    "generate": "tamagui generate-themes ./src/themes/theme.ts ./src/themes-generated.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.1",
    "@mintter/shared": "*",
    "@tamagui/animations-moti": "1.90.2",
    "@tamagui/animations-react-native": "1.90.2",
    "@tamagui/cli": "1.90.2",
    "@tamagui/config": "1.90.2",
    "@tamagui/create-theme": "1.90.2",
    "@tamagui/font-inter": "1.90.2",
    "@tamagui/helpers-icon": "1.90.2",
    "@tamagui/lucide-icons": "1.90.2",
    "@tamagui/react-native-media-driver": "1.90.2",
    "@tamagui/shorthands": "1.90.2",
    "@tamagui/theme-builder": "1.90.2",
    "@tamagui/themes": "1.90.2",
    "@tamagui/web": "1.90.2",
    "react": "18.2.0",
    "react-native-reanimated": "^3.7.0",
    "react-virtuoso": "4.6.2",
    "tamagui": "1.90.2"
  },
  "devDependencies": {
    "@mintter/prettier": "*",
    "@tamagui/build": "1.90.2"
  }
}
