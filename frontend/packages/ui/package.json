{
  "name": "@mintter/ui",
  "version": "0.1.0",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "generate": "tamagui generate-themes ./src/themes.ts ./src/generated-themes.ts",
    "lint": "eslint .",
    "build": "tamagui-build",
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json}\" --check --ignore-path ../../../.prettierignore",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "1.52.5",
    "@tamagui/config": "1.52.5",
    "@tamagui/create-theme": "^1.53.1",
    "@tamagui/font-inter": "1.52.5",
    "@tamagui/lucide-icons": "1.52.5",
    "@tamagui/react-native-media-driver": "1.52.5",
    "@tamagui/shorthands": "1.52.5",
    "@tamagui/theme-builder": "^1.53.1",
    "@tamagui/themes": "1.52.5",
    "@tamagui/toast": "1.52.5",
    "@tamagui/web": "1.52.5",
    "tamagui": "1.52.5"
  },
  "devDependencies": {
    "@mintter/prettier": "workspace:*",
    "@tamagui/build": "1.52.5",
    "@tamagui/cli": "1.52.5",
    "@tamagui/generate-themes": "1.52.5",
    "eslint-config-custom": "workspace:*"
  }
}
